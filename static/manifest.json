{
    "manifest_version": 3,
    "name": "Scrapper",
    "version": "1.0.0",
    "background":{
        "service_worker":"background.js"
    },
    "commands": {
        "reload": {
          "suggested_key": "Ctrl+Shift+Y",
          "description": "reload extension"
        }
    },
    "action":{
        "default_title":"Click me"
    },
    "permissions": ["scripting", "activeTab"]
}